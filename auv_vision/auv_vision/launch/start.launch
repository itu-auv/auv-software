<launch>
  <arg name="namespace" default="taluy" />
  <group ns="$(arg namespace)">
    <node pkg="auv_vision" type="front_bottom_yolo_handler.py" name="yolo_handler_node" output="screen">
      <remap from="cam_front/image_raw" to="cameras/cam_front/image_raw" />
      <remap from="cam_bottom/image_raw" to="cameras/cam_bottom/image_raw" />
    </node>
  </group>
</launch>
