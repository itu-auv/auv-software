<?xml version="1.0"?>
<launch>
  <include file="$(find auv_detection)/launch/inc/tracker_segment.launch" />
  <include file="$(find auv_detection)/launch/inc/tracker_detection.launch" />

  <!-- YOLO mask node -->
  <node pkg="auv_detection"
        type="yolo_to_mask_node.py"
        name="yolo_seg_to_pipe_mask"
        output="screen" />

  <!-- Pipe follower node -->
  <node pkg="auv_detection"
        type="pipe_follower_node.py"
        name="pipe_follower_node"
        output="screen" />
</launch>
