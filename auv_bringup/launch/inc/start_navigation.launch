<?xml version="1.0"?>
<launch>
  <arg name="namespace" default="taluy" />
  <arg name="control_rate" default="20.0" />
  <arg name="use_gui" default="false" />
  <arg name="config_file" default="$(find auv_bringup)/config/$(arg namespace)/control.yaml" />

  <include file="$(find auv_bringup)/launch/inc/localization/start_auv_localization.launch">
    <arg name="namespace" value="$(arg namespace)" />
    <arg name="control_rate" value="$(arg control_rate)" />
  </include>

  <include file="$(find auv_bringup)/launch/inc/localization/start_robot_localization.launch.xml">
    <arg name="namespace" value="$(arg namespace)" />
    <arg name="control_rate" value="$(arg control_rate)" />
  </include>

  <include file="$(find auv_bringup)/launch/inc/start_control.launch">
    <arg name="namespace" value="$(arg namespace)" />
    <arg name="rate" value="$(arg control_rate)" />
    <arg name="use_gui" value="$(arg use_gui)" />
    <arg name="config_file" value="$(arg config_file)"/>
  </include>

  <include file="$(find auv_bringup)/launch/inc/start_mapping.launch">
    <arg name="namespace" value="$(arg namespace)" />
    <arg name="control_rate" value="$(arg control_rate)" />
  </include>

  <include file="$(find auv_bringup)/launch/inc/navigation/start_path_action_server.launch">
    <arg name="namespace" value="$(arg namespace)" />
  </include>

</launch>
