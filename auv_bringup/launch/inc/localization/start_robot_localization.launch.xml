<launch>
  <arg name="namespace" default="turquoise" />

  <group ns="$(arg namespace)">
    <node pkg="robot_localization" type="ekf_localization_node" name="auv_localization_node"
      clear_params="true">
      <rosparam command="load" file="$(find auv_bringup)/config/ekf.yaml" />
    </node>
  </group>
</launch>